{
  "name": "server",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-31",

  "durable_objects": {
    "bindings": [
      {
        "name": "MY_DURABLE_OBJECT",
        "class_name": "MyDurableObject"
      }
    ]
  },

  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["MyDurableObject"]
    }
  ],

  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "app-db",
      "database_id": "<PUT_ID_HERE>"
    }
  ]
}
